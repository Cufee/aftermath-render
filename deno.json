{
  "lock": false,
  "tasks": {
    "dev": "deno run -A --watch=components/,main.tsx main.tsx",
    "run": "deno run --allow-read --allow-env --allow-net main.tsx"
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "imports": {
    "satori": "npm:satori@0.10.11",
    "svg2png": "npm:svg2png-wasm@1.1.0",
    "preact": "npm:preact@10.19.2",
    "preact/": "https://esm.sh/preact@10.19.2/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "nodeModulesDir": false
}